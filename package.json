{
  "name": "react-webpack-typescript-starter",
  "version": "0.1.0",
  "description": "Starter kit for React, Webpack (with Hot Module Replacement), Typescript and Babel.",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "sass",
    "hmr",
    "starter",
    "boilerplate"
  ],
  "author": "Viktor Persson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vikpe/react-webpack-typescript-starter.git"
  },
  "bugs": {
    "url": "https://github.com/vikpe/react-webpack-typescript-starter/issues"
  },
  "homepage": "https://github.com/vikpe/react-webpack-typescript-starter",
  "scripts": {
    "build": "yarn run clean-dist && webpack -p --config=configs/webpack/prod.js",
    "clean-dist": "rm -f -r -d dist",
    "lint": "yarn run lint:ts && yarn run lint:sass",
    "lint:ts": "tslint './src/**/*.ts*' --format stylish --force",
    "lint:sass": "stylelint ./src/**/*.scss",
    "start": "yarn run start-dev",
    "start-dev": "webpack-dev-server --config=configs/webpack/dev.js",
    "start-prod": "yarn run build && node express.js",
    "test": "jest --watch --coverage --config=configs/jest.json"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@blueprintjs/core": "^3.5.1",
    "@types/classnames": "^2.2.6",
    "@types/enzyme": "^3.1.13",
    "@types/i18next": "^8.4.5",
    "@types/i18next-browser-languagedetector": "^2.0.1",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.7.1",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "@types/react-i18next": "^7.8.2",
    "@types/react-test-renderer": "^16.0.2",
    "@types/recompose": "^0.26.4",
    "@types/uuid": "^3.4.4",
    "awesome-typescript-loader": "^5.2.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "classnames": "^2.2.6",
    "css-loader": "^1.0.0",
    "date-fns": "^2.0.0-alpha.16",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "gun": "^0.9.99993",
    "html-webpack-plugin": "^3.2.0",
    "i18next": "^11.7.0",
    "i18next-browser-languagedetector": "^2.2.3",
    "image-webpack-loader": "^4.3.1",
    "jest": "^23.5.0",
    "node-sass": "^4.9.3",
    "react": "^16.4.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.4",
    "react-i18next": "^7.11.1",
    "react-test-renderer": "^16.5.0",
    "recompose": "^0.30.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "uglifyjs-webpack-plugin": "^2.0.0",
    "uuid": "^3.3.2",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {}
}
